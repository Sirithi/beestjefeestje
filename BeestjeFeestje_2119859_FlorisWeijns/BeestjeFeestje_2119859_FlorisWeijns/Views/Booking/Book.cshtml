@model BookingCreateViewModelOne

<h1>Boek een beestje</h1>

<form method="post" asp-action="BookTwo">
    <div id="wizard">
        <input type="hidden" asp-for="Id" />
        <input type="hidden" asp-for="Animals" />
        <input type="hidden" asp-for="SelectedAnimals" />
        <input type="hidden" asp-for="Name" />
        <input type="hidden" asp-for="Email" />
        <input type="hidden" asp-for="PhoneNumber" />
        <input type="submit" class="btn btn-primary pull-right" value="Boek"></input>
        <ul>
            <li>
                <a>Kies een datum</a>
            </li>
        </ul>
        <div class="tab-content">
            <div id="date">
                <input type ="date" asp-for="Date"/>
            </div>
        </div>
    </div>
</form>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}